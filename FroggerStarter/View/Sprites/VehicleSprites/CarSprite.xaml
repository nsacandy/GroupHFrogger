        <Rectangle Canvas.Top="10" Canvas.Left="10" Width="10" Height="30" Fill="Gray" />
        <Rectangle Canvas.Top="10" Canvas.Left="40" Width="10" Height="30" Fill="Blue" />
    </Canvas>
