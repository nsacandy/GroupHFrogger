<local:CarSprite
    x:Class="FroggerStarter.View.Sprites.VehicleSprites.SportsCarSprite"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:FroggerStarter.View.Sprites.VehicleSprites"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:vehicleSprites="using:FroggerStarter.View.Sprites.VehicleSprites"
    mc:Ignorable="d"
    Height="50"
    Width="50">


    <Canvas>
        <vehicleSprites:CarSprite />
        <Rectangle Fill="AliceBlue" Canvas.Left="0" Canvas.Top="20" Width="10" Height="10" />
        <Rectangle Fill="AliceBlue" Canvas.Left="20" Canvas.Top="20" Width="30" Height="10" />
    </Canvas>
</local:CarSprite>